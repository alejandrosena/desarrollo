<%= simple_form_for @register, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :date %>
  <%= f.input :time %>
  <%= f.input :amount %>
  <%= f.input :movement %>
  <%= f.input :destination %>
  <%= f.input :procedensia %>
  <%= f.input :center %>
  <%= f.input :vehicle_id %>
  <%= f.input :porter_id %>
  <%= f.input :material_id %>
  <%= f.input :user_id %>
  <%= f.input :team_id %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                registers_path, :class => 'btn' %>
  </div>
<% end %>
