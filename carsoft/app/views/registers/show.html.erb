<%- model_class = Register -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= @register.date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:time) %>:</strong></dt>
  <dd><%= @register.time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:amount) %>:</strong></dt>
  <dd><%= @register.amount %></dd>
  <dt><strong><%= model_class.human_attribute_name(:movement) %>:</strong></dt>
  <dd><%= @register.movement %></dd>
  <dt><strong><%= model_class.human_attribute_name(:destination) %>:</strong></dt>
  <dd><%= @register.destination %></dd>
  <dt><strong><%= model_class.human_attribute_name(:procedensia) %>:</strong></dt>
  <dd><%= @register.procedensia %></dd>
  <dt><strong><%= model_class.human_attribute_name(:center) %>:</strong></dt>
  <dd><%= @register.center %></dd>
  <dt><strong><%= model_class.human_attribute_name(:vehicle_id) %>:</strong></dt>
  <dd><%= @register.vehicle_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:porter_id) %>:</strong></dt>
  <dd><%= @register.porter_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:material_id) %>:</strong></dt>
  <dd><%= @register.material_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @register.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:team_id) %>:</strong></dt>
  <dd><%= @register.team_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              registers_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_register_path(@register), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              register_path(@register),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
